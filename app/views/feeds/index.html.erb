<h1>Listing feeds</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Notes</th>
    <th>Stage feed url</th>
    <th>Production feed url</th>
    <th>Production start date</th>
    <th>Production end date</th>
    <th>Company</th>
    <th>Feed status</th>
    <th>Feed frequency</th>
    <th>Feed type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @feeds.each do |feed| %>
  <tr>
    <td><%= feed.name %></td>
    <td><%= feed.notes %></td>
    <td><%= feed.stage_feed_url %></td>
    <td><%= feed.production_feed_url %></td>
    <td><%= feed.production_start_date %></td>
    <td><%= feed.production_end_date %></td>
    <td><%= feed.company %></td>
    <td><%= feed.feed_status %></td>
    <td><%= feed.feed_frequency %></td>
    <td><%= feed.feed_type %></td>
    <td><%= link_to 'Show', feed %></td>
    <td><%= link_to 'Edit', edit_feed_path(feed) %></td>
    <td><%= link_to 'Destroy', feed, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Feed', new_feed_path %>
